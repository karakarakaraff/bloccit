<h1>All Questions <%= link_to "New Question", new_question_path(@question), class: 'btn btn-success' %></h1>
<% @questions.each do |question| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= index_resolved(question) %>
      </h4>
    </div>
  </div>
<% end %>
